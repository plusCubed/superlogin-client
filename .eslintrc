parserOptions:
    ecmaVersion: 6
    sourceType: module

env:
    browser : true

extends: airbnb-base

rules:
    no-underscore-dangle: off
    comma-dangle: off

    arrow-parens: [error, as-needed]
    no-param-reassign: [error, { props: false }]

    max-len:
        - error
        - 120
        - { ignoreUrls: true, ignoreComments: false }
